{
  "name": "wsc-reports",
  "version": "1.0.0",
  "description": "Api for report section",
  "main": "index.js",
  "dependencies": {
    "babelify": "^7.3.0",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "file-type": "^5.2.0",
    "http-proxy": "^1.16.2",
    "minimist": "^1.2.0",
    "morgan": "^1.8.2",
    "node-cache": "^4.1.1",
    "npm-run-all": "^4.0.2",
    "oracledb": "^1.13.1",
    "performance-now": "^2.1.0",
    "watchify": "^3.9.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm-run-all -s js-build -p start-react-prod start-prod-server",
    "dev": "npm-run-all -p js-watch start-react-dev start-dev-server",
    "js-build": "cross-env NODE_ENV=production nodemon -w build/build.js -i [] build/build.js",
    "js-watch": "cross-env NODE_ENV=development nodemon -w build/build.js -i [] build/build.js",
    "start-prod-server": "cross-env NODE_ENV=production node server.js",
    "start-dev-server": "cross-env NODE_ENV=development nodemon --inspect server.js",
    "start-react-prod": "node build/run-react.js build",
    "start-react-dev": "node build/run-react.js start",
    "postinstall": "cd ./react && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qhadron/wsc-reports.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qhadron/wsc-reports/issues"
  },
  "homepage": "https://github.com/qhadron/wsc-reports#readme"
}
