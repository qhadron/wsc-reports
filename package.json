{
  "name": "wsc-reports",
  "version": "1.0.0",
  "description": "Api for report section",
  "main": "server.js",
  "dependencies": {
    "babelify": "^7.3.0",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "file-saver": "^1.3.3",
    "file-type": "^5.2.0",
    "http-proxy": "^1.16.2",
    "minimist": "^1.2.0",
    "morgan": "^1.8.2",
    "node-cache": "^4.1.1",
    "npm-run-all": "^4.0.2",
    "oracledb": "^1.13.1",
    "performance-now": "^2.1.0",
    "watchify": "^3.9.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.4.1",
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prod": "npm-run-all -p js-build start-ui-prod start-server-prod",
    "dev": "npm-run-all -p js-watch start-ui-dev start-server-dev",
    "js-build": "cross-env NODE_ENV=production node build/build.js",
    "js-watch": "cross-env NODE_ENV=development nodemon -w build/build.js -i [] build/build.js",
    "start-server-prod": "cross-env NODE_ENV=production node server.js",
    "start-server-dev": "cross-env NODE_ENV=development nodemon --inspect server.js",
    "start-ui-prod": "cross-env NODE_ENV=production node build/run-ui.js ",
    "start-ui-dev": "cross-env NODE_ENV=development node build/run-ui.js ",
    "postinstall": "cd ui && npm install",
    "start": "npm-rul-all prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qhadron/wsc-reports.git"
  },
